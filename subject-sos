      <div style="display: visible" class="results-container">



        <div v-if="isFetchInProgress">
          <div class="px-5 py-4 d-flex justify-content-start"
            style="background-color: #f2f3f5; border-bottom-left-radius: 8px; border-bottom-right-radius: 8px">
