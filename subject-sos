<script src="/assets/js/lib/vue@2.6.14.min.js"></script>
<script src="https://www.google.com/recaptcha/api.js?render=6LdNSeodAAAAAJlFvgy3Apoo9Hm5mupppRPJo0hB"></script>
<script src="/assets/js/lib/jquery@3.6.0.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.18.0/axios.js"></script>
<script>
  var app = new Vue({
    el: '#app',
    data: function () {
      return {
        results: [],
        subject: null,
        isFetchInProgress: false,
        isErrorWhenFetching: false,
      };
    },

    mounted: function () {
      var subject = this.getSubjectFromURL();
      if (subject) {
        this.subject = subject;
        this.fetchBackend();
      }
    },

    updated: function () {
      try {
        $('[data-toggle="tooltip"]').tooltip();

        $('.clipboard-copy').each(function () {
          $(this).click(function (e) {
            var _this = $(this);

            _this.attr('data-original-title', 'Copied!').tooltip('update').tooltip('show');

            setTimeout(function () {
              _this.attr('data-original-title', 'Copy to clipboard').tooltip('update').tooltip('hide');
            }, 1000);
          });
        });
      } catch (e) { }
    },

    methods: {
      onSubmitTest: function () {
        this.errors = [];
        if (!this.subject) {
          this.errors.push('Please enter your email subject');
          return;
        }

        this.fetchBackend();
        this.setSubjectFromURL();
      },

      fetchBackend: function () {
        const results = [];
        this.results = results;

        if (this.isFetchInProgress) return;
        this.isFetchInProgress = true;
        this.isErrorWhenFetching = false;

        // First subject bellow
        const mySubjectLine = {
          sentence: this.subject,
        };

        const baseUrl = window.location.href.indexOf('mailmeteor.com') > 0 ? 'https://mailmeteor-free-tools-4zqxxgfnaq-ue.a.run.app' : 'http://localhost:8080';

        axios
          .post(`${baseUrl}/api/subject_line_`, mySubjectLine)
          .then((res) => {
            const resArray = res.data;

            resArray.forEach((element) => {
              results.push(element);
            });

            this.results = results;
            if (res) {
              this.isFetchInProgress = false;
            } else {
              this.isFetchInProgress = false;
            }

          })
          .catch(function (err) {
            console.error(err);
            console.log("je passe par catch");
            if (err) {
              this.isErrorWhenFetching = true;
            }
            console.log(this.isErrorWhenFetching);
          })
          .finally((err) => {
            this.isFetchInProgress = false;
            /*if (err) {
              this.isErrorWhenFetching = true;
            } else {
              this.isErrorWhenFetching = false;
            }*/
            this.mySubjectLine = "";
          });
        console.log(this.isErrorWhenFetching);
      },

      onClickAlternatives: function (someResult) {
        this.subject = someResult;
        const mySubjectLine = {
          sentence: this.subject,
        };
        this.onSubmitTest(mySubjectLine);
      },

      scoreClassname: function (score) {
        return score > 74 ? 'good-score' : score <= 74 && score >= 55 ? 'average-score' : 'bad-score';
      },

      setSubjectFromURL: function () {
        var url = new URL(window.location);
        url.searchParams.set('subject', this.subject);
        window.history.pushState('', '', url);
      },

      getSubjectFromURL: function () {
        var url = new URL(window.location);
        return url.searchParams.get('subject');
      },

      onCopyResult: function (sentence) {
        try {
          navigator.clipboard.writeText(sentence);
        } catch (err) {
          /* do nothing */
        }
      },
    },
  });
</script>

